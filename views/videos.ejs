<% layout('layouts/boilerplate') %>

<div>

  <section class="py-5 px-2 text-center bg-primary text-light section-shape-right">
    <div class="row py-lg-3">
      <div class="col-lg-6 col-md-8 mx-auto">
        <h1 class="fw-light">Past Concerts</h1>
        <h5 class="lead text-dark">Missed a performance or want to see it again? This is the place!</h5>
      </div>
    </div>
  </section>

  <div class="container p-5">
    <div class="row" data-aos="fade-up" data-aos-duration="2000">
      <div class="col-md-7">
        <h2 class="fw-normal lh-1">Illuminare</h2>
        <h4>June 1, 2024</h4>
        <p class="lead text-body-secondary">Lake Grove’s Sanctuary Choir’s spring concert featured Illuminare, a beautiful new piece by Elaine Hagenberg, one of the choir’s favorite composers. The choir was accompanied by members of the Oregon Symphony. </p>
      </div>
      <div class="col-md-5">
        <div class="ratio ratio-16x9">
          <iframe width="560" height="315" src="https://www.youtube.com/embed/7ZrvZjRYKz4?si=LGY1OD0akEClej5q" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen></iframe>
        </div>
      </div>
    </div>
  </div>

